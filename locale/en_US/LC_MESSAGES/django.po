msgid "ЛОДКА"
msgstr "A BOAT"

#: .\templates\index\index.html:123
msgid "СПОРТИВНЫЙ ИНВЕНТАРЬ"
msgstr "SPORTS EQUIPMENT"

#: .\templates\index\index.html:129
